## enumerate a list
subjects = ('skiing', 'is', 'cool')
for i, subject in enumerate(subjects):
    print(i, subject)
    
## initialize an empty list (set)
items = set()
# Add three strings.
items.add("biking")
items.add("skiing")
items.add("hiking")
print(items)

## create an immutable list (set)
objects = {"biking", "skiing", "hiking", "boating"}
print(len(objects))

 # check if list (set) contains item
 if "biking" in objects:
 if "skiing" not in items:
 
