first_word = split_string[0]
last_word = split_string[-1]

## remove first word from string
string = string.split(' ', 1)[1]

## remove last word from string
string = string.rsplit(' ', 1)[0]

## look for blank spaces within a string/field, with no characters
if str.isspace(): # contains space(s)
if not str.isspace(): # does not contain space(s)

## using 'in' within an 'if statement'
if myVal in ("N","S","E","W"):

## look for a digit within a string
if any(char.isdigit() for char in myString):

